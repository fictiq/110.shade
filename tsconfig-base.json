{
  "compilerOptions": {
    //  "declaration": true,
    "module": "commonjs",
    "target": "es2017",
    "incremental": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": false,
    "sourceMap": true,
    "skipLibCheck": false,
    "outDir": "dist",
    "baseUrl": "."
  }
}
